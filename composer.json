{
    "name": "skydiver/nova-update-card",
    "description": "Check if you're running the latest versions of Laravel or Nova right from your dashboard.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "composer/semver": "^3.2",
        "paquettg/php-html-parser": "^3.1"
    },
    "require-dev": {
      "orchestra/testbench": "^6.1",
      "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "Skydiver\\NovaUpdateCard\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Skydiver\\NovaUpdateCard\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Skydiver\\NovaUpdateCard\\CardServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
